{
    "name": "birdsinthesun/fly_ux",
    "type": "contao-bundle",
    "description": "Entfernt die Artikel-Ebene in Contao CMS und optimiert die UX. Achtung: Noch in der Entwicklung",
    "license": "MIT",
    "version":"1.1.1",
    "authors": [
        {
            "name": "Monique Hahnefeld",
            "email": "hello@bits-design.de"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/routing":"^6.2",
        "contao/core-bundle": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Bits\\FlyUxBundle\\": "src/"
        }
    },
  "extra": {
        "contao-manager-plugin": "Bits\\FlyUxBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
    "post-install-cmd": [
      "php bin/console asset:install --relative"
    ],
    "post-update-cmd": [
      "php bin/console asset:install --relative"
    ]
  }
    
    
}
